{
    "name": "dotsunited/bundlefu-silex-service-provider",
    "description": "Silex service provider for BundleFu",
    "keywords": ["silex", "service", "provider", "css", "javascript", "bundling", "asset"],
    "homepage": "https://github.com/dotsunited/BundleFuSilexServiceProvider",
    "license": "MIT",
    "authors": [
        {
            "name": "Jan Sorgalla",
            "email": "jan.sorgalla@dotsunited.de",
            "homepage": "http://dotsunited.de"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "dotsunited/bundlefu": "1.*"
    },
    "require-dev": {
        "silex/silex": "1.0.*",
        "dotsunited/bundlefu-twig-extension": "1.*"
    },
    "suggest": {
        "dotsunited/bundlefu-twig-extension": "Provides integration with the Twig templating engine"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "DotsUnited\\BundleFu\\Silex": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
